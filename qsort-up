#include <stdio.h>
#include <string.h>

void PrintArray(double A[], int size) {
	for (int i = 0; i < size; i++) {
		printf("%5.1f", A[i]);
	}
	printf("\n");
}

int Compare(double* x, double * y) {
	return *x - *y;
}

int main(void) {
	double A[] = { 10.0, 5.0, 20.0, 50.0, 35.0, 45.0, 15.0, 25.0, 30.0, 40.0 };
	PrintArray(A, 10);
	qsort(A, 10, sizeof(double), Compare);
	PrintArray(A, 10);

	
}

 
